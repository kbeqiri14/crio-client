{
  "name": "crio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.4.15",
    "@aws-cdk/aws-certificatemanager": "^1.124.0",
    "@aws-cdk/aws-cloudfront": "^1.124.0",
    "@aws-cdk/aws-route53": "^1.124.0",
    "@aws-cdk/aws-route53-targets": "^1.124.0",
    "@aws-cdk/aws-s3": "^1.124.0",
    "@aws-cdk/core": "^1.124.0",
    "@craco/craco": "^6.3.0",
    "@sentry/react": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@tidepoollabs/auth": "^1.0.0",
    "@tidepoollabs/react-manage-users": "^1.0.1",
    "@tidepoollabs/upload": "^1.0.0",
    "antd": "^4.16.13",
    "aws-amplify": "^4.3.1",
    "aws-amplify-react": "^5.1.1",
    "aws-sdk": "^2.996.0",
    "axios": "^0.24.0",
    "classcat": "^5.0.3",
    "date-fns": "^2.26.0",
    "graphql": "^15.6.0",
    "history": "^4.10.1",
    "lodash": "^4.17.21",
    "normalize.less": "^1.0.0",
    "pluralize": "^8.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-dotdotdot": "^1.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.17.0",
    "react-image": "^4.0.3",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "swiper": "^6.8.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/node-logger": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "browserslist": "^4.17.3",
    "craco-alias": "^3.0.1",
    "craco-antd": "^1.19.0",
    "craco-less": "^1.20.0",
    "crio-scripts": "file:crio-scripts",
    "env-cmd": "^10.1.0",
    "husky": "^7.0.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "serve": "^12.0.1",
    "storybook-preset-craco": "0.0.6",
    "terser": "^5.9.0"
  },
  "scripts": {
    "prepare": "npx husky install",
    "preinstall": "npm i --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions",
    "generate-config": "node generateCdkConfig.js",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "start": "crio-scripts start",
    "build": "craco build --max-old-space-size=4096",
    "test": "craco test --passWithNoTests",
    "lint": "eslint ./src --ext .js --max-warnings 0",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|less|md)\"",
    "sb": "start-storybook -p 6006 -s public",
    "build-sb": "build-storybook -s public"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "browserslist": "4.17.3",
    "node-fetch": "2.6.1",
    "immer": "9.0.6",
    "set-value": "4.0.1",
    "nth-check": "2.0.1",
    "ansi-regex": "5.0.1",
    "trim": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint *.js --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
