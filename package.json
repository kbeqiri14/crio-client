{
  "name": "crio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.6.9",
    "@aws-cdk/aws-certificatemanager": "^1.172.0",
    "@aws-cdk/aws-cloudfront": "^1.172.0",
    "@aws-cdk/aws-route53": "^1.172.0",
    "@aws-cdk/aws-route53-targets": "^1.172.0",
    "@aws-cdk/aws-s3": "^1.172.0",
    "@aws-cdk/core": "^1.172.0",
    "@craco/craco": "^6.4.5",
    "@sentry/react": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tidepoollabs/auth": "^2.0.0",
    "@tidepoollabs/react-manage-users": "^1.1.0",
    "@tidepoollabs/upload": "^1.0.0",
    "antd": "^4.23.1",
    "aws-amplify": "^4.3.35",
    "aws-sdk": "^2.1213.0",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.0",
    "date-fns": "^2.29.2",
    "graphql": "^16.6.0",
    "history": "^4.10.1",
    "normalize.less": "^1.0.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.35.0",
    "react-horizontal-scrolling-menu": "^3.2.0",
    "react-player": "^2.10.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@react-theming/storybook-addon": "^1.1.7",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "browserslist": "^4.21.3",
    "craco-alias": "^3.0.1",
    "craco-antd": "^1.19.0",
    "crio-scripts": "file:crio-scripts",
    "env-cmd": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "scripts": {
    "prepare": "npx husky install",
    "preinstall": "npm i --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions",
    "generate-config": "node generateCdkConfig.js",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "start": "crio-scripts start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider && craco build --max-old-space-size=4096",
    "test": "craco test --passWithNoTests",
    "lint": "eslint ./src --ext .js --max-warnings 0",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|less|md)\"",
    "sb": "start-storybook -p 6006 -s public",
    "build-sb": "build-storybook -s public"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "browserslist": "4.17.3",
    "node-fetch": "2.6.1",
    "immer": "9.0.6",
    "set-value": "4.0.1",
    "nth-check": "2.0.1",
    "ansi-regex": "5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint *.js --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
