{
  "name": "crio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.4.15",
    "@aws-cdk/aws-certificatemanager": "^1.124.0",
    "@aws-cdk/aws-cloudfront": "^1.124.0",
    "@aws-cdk/aws-route53": "^1.124.0",
    "@aws-cdk/aws-route53-targets": "^1.124.0",
    "@aws-cdk/aws-s3": "^1.124.0",
    "@aws-cdk/core": "^1.124.0",
    "@craco/craco": "^6.3.0",
    "@sentry/react": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@tidepoollabs/auth": "^1",
    "@tidepoollabs/react-manage-users": "^1.0.1",
    "@tidepoollabs/upload": "^1",
    "antd": "^4.16.13",
    "aws-amplify": "^4.3.1",
    "aws-amplify-react": "^5.1.1",
    "aws-sdk": "^2.996.0",
    "classcat": "^5.0.3",
    "graphql": "^15.6.0",
    "history": "^5.0.1",
    "normalize.less": "^1.0.0",
    "pluralize": "^8.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.2",
    "react-dotdotdot": "^1.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.17.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "craco-alias": "^3.0.1",
    "craco-antd": "^1.19.0",
    "craco-less": "^1.20.0",
    "husky": "^7.0.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "serve": "^12.0.1",
    "terser": "^5.9.0"
  },
  "scripts": {
    "generate-config": "node generateCdkConfig.js",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "start": "nodemon -w craco.config.js -w jsconfig.json --exec \"craco start\"",
    "build": "craco build --max-old-space-size=4096",
    "test": "craco test --passWithNoTests",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|less|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint *.js --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
